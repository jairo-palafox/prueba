			<xsd:element name="fechaPresentacion" nillable="true" type="xsd:string" />
			<xsd:element name="nombres" nillable="true" type="xsd:string" />
			<xsd:element name="nss" nillable="true" type="xsd:string" />
			<xsd:element name="numCreditoInfonavit" nillable="true" type="xsd:string" />
			<xsd:element name="rfc" nillable="true" type="xsd:string" minOccurs="0" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SolicDesmarcaRespVO">
		<xsd:sequence>
			<xsd:element name="apeMaternoBD" nillable="true" type="xsd:string" minOccurs="0" />
			<xsd:element name="apeMaternoInfo" nillable="true" type="xsd:string" minOccurs="0" />
			<xsd:element name="apePaternoBD" nillable="true" type="xsd:string" />
			<xsd:element name="apePaternoInfo" nillable="true" type="xsd:string" />
			<xsd:element name="diagProceso" nillable="true" type="xsd:string" />
			<xsd:element name="nombresBD" nillable="true" type="xsd:string" />
			<xsd:element name="nombresInfo" nillable="true" type="xsd:string" />
			<xsd:element name="nss" nillable="true" type="xsd:string" />
			<xsd:element name="resultOperacion" nillable="true" type="xsd:string" />
			<xsd:element name="rfc" nillable="true" type="xsd:string" minOccurs="0" />
			<xsd:element name="rfcBD" nillable="true" type="xsd:string" minOccurs="0" />
			<xsd:element name="tipCreditDesm" nillable="true" type="xsd:string" />
		</xsd:sequence>
	</xsd:complexType>

</xsd:schema>